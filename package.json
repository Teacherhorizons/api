{
  "name": "acme-api",
  "version": "1.0.0",
  "dependencies": {
    "@redocly/cli": "^1.0.0-beta.102",
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "jest": "^28.0.0",
    "jest-openapi": "^0.14.2"
  },
  "private": true,
  "scripts": {
    "start": "openapi preview-docs -p 8080",
    "build-yaml": "openapi bundle --output=tests/specs/openapi.yaml --dereferenced",
    "build-json": "openapi bundle --output=tests/specs/openapi.json --dereferenced",
    "validate": "openapi lint",
    "lint": "openapi lint",
    "pretest": "npm run build-yaml",
    "test": "jest --watchAll=false",
    "jest": "jest --watchAll=false",
    "lint--generate-ignore-file": "openapi lint --generate-ignore-file"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.2",
    "babel-jest": "^28.1.1",
    "ts-jest": "^28.0.5"
  }
}