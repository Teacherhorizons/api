{
  "name": "acme-api",
  "version": "1.0.0",
  "dependencies": {
    "@redocly/cli": "^1.0.0-beta.96"
  },
  "private": true,
  "scripts": {
    "start": "openapi preview-docs -p 8080",
    "build": "openapi bundle --output=dist",
    "build-wip-2": "openapi bundle --output=dist/openapi.yaml --dereferenced",
    "build-wip-3": "openapi bundle --output=dist/openapi.json --dereferenced",
    "build-wip-4": "openapi bundle --output=dist/openapi.json",
    "test": "openapi lint",
    "lint": "openapi lint",
    "lint--generate-ignore-file": "openapi lint --generate-ignore-file"
  }
}