{
  "name": "acme-api",
  "version": "1.0.0",
  "dependencies": {
    "@redocly/cli": "^1.0.0-beta.102",
    "axios": "^0.27.2",
    "cookie": "^0.5.0",
    "form-data": "^4.0.0",
    "jest": "^28.0.0",
    "jest-openapi": "^0.14.2",
    "shelljs": "^0.8.5"
  },
  "private": true,
  "scripts": {
    "start": "redocly preview-docs -p 8080",
    "build-yaml": "redocly bundle --output=tests/specs/openapi.yaml --dereferenced",
    "build-json": "redocly bundle --output=tests/specs/openapi.json --dereferenced",
    "validate": "redocly lint",
    "lint": "redocly lint",
    "lint--generate-ignore-file": "redocly lint --generate-ignore-file",
    "test": "jest --watchAll=false --unhandled-rejections=strict",
    "jest": "npm run test",
    "prettify-ts": "npx prettier --write **/*.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.2",
    "@types/shelljs": "^0.8.11",
    "babel-jest": "^28.1.1",
    "ts-jest": "^28.0.5"
  }
}