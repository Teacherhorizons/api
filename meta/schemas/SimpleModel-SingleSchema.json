      },
      "required": ["type", "additionalProperties", "properties", "required"],
      "title": "SimpleModel-SingleSchema-Schema"
    },
    "SimpleModel-SingleSchema-Id": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
