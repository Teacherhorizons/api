          "const": "integer"
        },
        "example": {
          "type": "number",
          "const": 1
        },
        "readOnly": {
          "type": "boolean",
          "const": true
        },
        "description": {
          "type": "string",
          "const": "Unique identifier of the object"
        }
      },
      "required": ["type", "example", "readOnly", "description"],
      "title": "SimpleModel-SingleSchema-Id"
    }
