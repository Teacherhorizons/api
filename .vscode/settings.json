{
  "openapi.defaultPreviewRenderer": "redoc",
  "yaml.schemas": {
    "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/schemas/v3.0/schema.json": ["openapi.yaml"],
    "meta/schemas/LibraryModel.json": ["/openapi/components/models/library/*/*.yaml"],
    "meta/schemas/SimpleModel-SingleSchema.json": ["/openapi/components/models/simple/*/*.yaml"],
    "meta/schemas/JsonApiModel-SingleSchema.json": ["/openapi/components/models/json-api/*/*.yaml"],
    "meta/schemas/Response-MultipleSchemas.json": ["/openapi/components/responses/*/*.yaml"],
    "meta/schemas/Path-Plural.json": ["/openapi/paths/*/*.yaml"]
  }
}
